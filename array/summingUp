import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
    // write your code here
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int sum =0;
    int [] arr = new int[n];
    for(int i=0; i<n; i++){
        arr[i] = scn.nextInt();
    }
    
    //syntax for displaying array
    for(int i=0; i<n; i++){
        System.out.print(arr[i] + " ");
    }
    
    System.out.println();
    //syntax for summing up the array
    for(int i=0; i<n; i++){
        sum += arr[i];
    }
    
    System.out.println("Sum of all the elements of the arrays is : " + sum);
    
    
    
 }

}
