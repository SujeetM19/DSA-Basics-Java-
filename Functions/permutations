import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        int r = scn.nextInt();
        int answer = factorial(n)/ factorial(n-r);
        System.out.println(answer);
    }
    
    public static int factorial(int num){
        int ans = 1;
        for(int i=1; i<=num;i++)
        {
            ans*=i;
        }
        return ans;
    }

}
